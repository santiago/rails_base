#home.content
  -if @partner.partner_link_request_rejected?
    = render "partner_link_request_rejected"

  -if @partner_link_request_code
    =render 'partner_link_request'
  -if session[:has_nsbg]
    = render "heid"
  -else
    =render "cyn_introduction"
  #wylayp
    .header
      %h1 What You Love About Your Partner
    #wylap-content
      .wrapper
        %textarea{:name=>"wylayp_message_text", :cols => 70, :readonly=>"readonly"}
          #{@partner_wylayp[0].nil? ? nil : @partner_wylayp[0].message_text}
        %p And...
        %textarea{:name=>"wylayp_message_text_2", :cols => 70, :readonly=>"readonly"}
          #{@partner_wylayp[1].nil? ? nil : @partner_wylayp[1].message_text}
        %p And...
        %textarea{:name=>"wylayp_message_text_3", :cols => 70, :readonly=>"readonly"}
          #{@partner_wylayp[2].nil? ? nil : @partner_wylayp[2].message_text}
  =render "photo_gallery"  
